{
  "name": "archon",
  "version": "0.1.0",
  "description": "Automation Testing Framework",
  "main": "/lib/core/client.js",
  "scripts": {
    "lint": "eslint core test --ext .js --ignore-path .gitignore --fix",
    "docs": "npm run create-docs && open docs/index.html",
    "create-docs": "esdoc -c esdoc.json",
    "create-tutorials": "",
    "nuke": "npm cache clean && rimraf node_modules && yarn install",
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --compilers js:babel-core/register"
  },
  "author": "Derick Warshaw",
  "license": "unlicensed",
  "dependencies": {
    "appium": "^1.6.3",
    "babel-polyfill": "^6.1.18",
    "browser-sync": "^2.18.6",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^3.0.1",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "~2.1.4",
    "selenium-standalone": "^6.0.1",
    "webdriverio": "4.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "eslint-plugin-import": "^1.8.1",
    "gulp-webdriver": "^2.0.3",
    "rimraf": "2.5.x",
    "esdoc": "^0.4.6",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "mochawesome": "^1.5.5"
  }
}
